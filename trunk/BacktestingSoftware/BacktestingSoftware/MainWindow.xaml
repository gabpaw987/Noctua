<Window x:Class="BacktestingSoftware.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Backtesting Software" Height="150" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="55"/>
            <RowDefinition Height="56" />
        </Grid.RowDefinitions>

        <WrapPanel Height="23" Orientation="Horizontal" HorizontalAlignment="Left" Margin="12,26,0,0" Name="wrapPanel1" VerticalAlignment="Top" Width="254">
            <TextBox Height="23" Name="AlgorithmTextBox" Width="163" Text="{Binding}"/>
            <Button Content="Button" Height="23" Name="AlgorithmButton" Width="75" Click="AlgorithmButton_Click" />
        </WrapPanel>
        <WrapPanel Height="23" HorizontalAlignment="Left" Margin="12,21,0,0" Name="wrapPanel2" VerticalAlignment="Top" Width="254" Grid.Row="1">
            <TextBox Height="23" Name="DataTextBox" Width="163" />
            <Button Content="Button" Height="23" Name="DataButton" Width="75" />
        </WrapPanel>
        <Label Content="Data" Grid.Row="2" Height="28" HorizontalAlignment="Left" Margin="12,0,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Algorithm" Height="28" HorizontalAlignment="Left" Margin="12,4,0,0" Name="label2" VerticalAlignment="Top" />
    </Grid>
</Window>